	keywords = {my\_papers},
	month = feb,
	note = {Citation Key Alias: lovelessTwoDecadesSpatiotemporal2016},
	pages = {19--30},
	title = {Two {Decades} of {Spatiotemporal} {Variations} in {Subduction} {Zone} {Coupling} {Offshore} {Japan}},
