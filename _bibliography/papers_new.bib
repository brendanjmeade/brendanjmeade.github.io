	doi = {10.1785/0120160059},
	file = {DeVries et al. - 2017 - Viscoelastic Block Models of the North Anatolian F.pdf:/Users/meade/Zotero/storage/UEY88S7S/DeVries et al. - 2017 - Viscoelastic Block Models of the North Anatolian F.pdf:application/pdf},
