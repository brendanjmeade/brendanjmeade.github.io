	number = {9},
	pages = {4205--4214},
	title = {Kinematically consistent models of viscoelastic stress evolution},
	url = {https://onlinelibrary.wiley.com/doi/10.1002/2016GL068375},
	urldate = {2022-04-23},
	volume = {43},
