	author = {Thompson, T. Ben and Plesch, Andreas and Shaw, John H. and Meade, Brendan J.},
	doi = {10.1002/2014GL062833},
	file = {Thompson et al. - 2015 - Rapid slip‐deficit rates at the eastern margin of .pdf:/Users/meade/Zotero/storage/BKFK4LMZ/Thompson et al. - 2015 - Rapid slip‐deficit rates at the eastern margin of .pdf:application/pdf},
